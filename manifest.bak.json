{
   "background": {
      "page": "background.html",
      "persistent": false
   },
   "browser_action": {
      "default_icon": "images/Gaia-tree.png",
      "default_popup": "popup.html"
   },
   "icons": {
        "128": "images/Gaia-tree.png"
    },
   "manifest_version": 2,
   "default_locale": "en",
   "description": "gaia jira",
   "name": "gaia jira",
   "permissions": [ "cookies", "tabs", "\u003Call_urls>", "clipboardWrite"],
   "version": "1.0.0",
   "content_scripts": [
       { 
           "js": ["libs/jquery-3.2.1.min.js", "js/jsonRpc.js", "js/content.js"],
            "matches": [ "file://*", "http://*/*", "https://*/*" ],
            "run_at": "document_idle",
            "all_frames": true
        } 
   ]
}
